@{
    ViewBag.Title = "Fill CV";
}

<div class="container">

    <h2>@ViewBag.Title</h2>

    <div class="row">
        <div class="col-lg-12">
            <div>

                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Personal details</a></li>
                    <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Education</a></li>
                    <li role="presentation"><a href="#messages" aria-controls="messages" role="tab" data-toggle="tab">Family</a></li>
                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="home">
                        Personal details
                        <br />
                        <div class="btn-group pull-right">
                            <div class="pull-right">
                                <button class="btn btn-primary">Next</button>
                            </div>
                        </div>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="profile">
                        Education
                        <br />
                        <div class="btn-group">
                            <button class="btn btn-warning">Previous</button>
                        </div>
                        <div class="btn-group pull-right">
                            <div class="pull-right">
                                <button class="btn btn-primary">Next</button>
                            </div>
                        </div>                        
                    </div>
                    <div role="tabpanel" class="tab-pane" id="messages">
                        Family
                        <br />
                        <div class="btn-group">
                            <button class="btn btn-warning">Previous</button>
                        </div>
                        <div class="btn-group pull-right">
                            <div class="pull-right">
                                <button class="btn btn-success">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

